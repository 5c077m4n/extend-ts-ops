{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "ES2022",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"useUnknownInCatchVariables": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"noEmit": true
	},
	"include": ["src/"]
}
